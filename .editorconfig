# EditorConfig is awesome: http://EditorConfig.org

# top-most EditorConfig file
root = true

# Windows-style newlines with a newline ending every file
[*]
end_of_line = crlf
insert_final_newline = true

# Set default charset and tab size
[*.{js,ts,tsx}]
charset = utf-8
indent_style = space
indent_size = 2
